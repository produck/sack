{
	"name": "@produck/sack",
	"version": "0.0.0",
	"description": "Resource abstract based HTTP client & designer for the browser and node.js by fetch API.",
	"files": [
		"src",
		"tsconfig.json"
	],
	"types": "src/index.d.ts",
	"main": "src/index.cjs",
	"type": "module",
	"exports": {
		"import": "./src/index.mjs",
		"require": "./src/index.cjs"
	},
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/produck/sack.git"
	},
	"scripts": {
		"test": "mocha \"**/*.spec.mjs\" -t 999999999",
		"build": "rollup -c script/rollup.config.mjs",
		"prepublishOnly": "npm run build"
	},
	"keywords": [
		"fetch",
		"http",
		"xhr"
	],
	"devDependencies": {
		"@rollup/plugin-node-resolve": "^15.2.3",
		"@rollup/plugin-terser": "^0.4.4",
		"@types/mocha": "^10.0.6",
		"@types/node": "^20.11.19",
		"mocha": "^10.3.0",
		"rollup": "^4.12.0"
	},
	"author": "ChaosLee",
	"license": "MIT",
	"dependencies": {
		"@produck/compose": "^0.1.0",
		"@produck/idiom-common": "^0.1.2"
	}
}
